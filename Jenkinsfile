            steps {
                sh 'echo "HELLO WORLD!!!"'
            }
        }

        stage('Test') {
            steps {
                sh 'echo "HELLO WORLD!!!"'
            }
        }

        stage('Deliver') {
            steps {
                sh 'echo "HELLO WORLD!!!"'
            }
        }
    }
}

